.video-link-testimonial {
    overflow: hidden;
    display: block;
    position: relative;
    color: #fff;
}

.popup-wrap {
    display: none;
    position: fixed;
    z-index: 9995;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0
}

.popup-wrap .popup-content {
    position: absolute;
    z-index: 2;
    top: 50%;
    left: 50%;
    margin: 0 auto;
    transform: translateY(-50%) translateX(-50%)
}

.popup-wrap .close-btn {
    position: absolute;
    top: -35px;
    right: -35px;
    background: #fff
}

.popup-wrap .close-btn:before,
.popup-wrap .close-btn:after {
    opacity: 1
}

.popup-wrap .popup-overlay {
    position: absolute;
    top: 0;
    left: 0;
    z-index: 1;
    width: 100%;
    height: 100%;
    background: rgba(255, 255, 255, .75)
}

.mobile .typeform-modal .typeform-close {
    top: 20px;
    right: 5px
}

.mobile .typeform-modal .typeform-close span {
    top: 12px
}

.video-link-testimonial {
    overflow: hidden;
    display: block;
    position: relative;
    color: #fff
}

.video-link-testimonial:before {
    content: '';
    position: absolute;
    top: 50%;
    left: 50%;
    z-index: 3;
    display: block;
    width: 160px;
    height: 160px;
    background-image: url(https://2n7n9l5nwvl1oaz22d5aak16-wpengine.netdna-ssl.com/wp-content/themes/new-tgs-theme/assets/css/../img/play-btn-hand.svg);
    background-position: center center;
    background-repeat: no-repeat;
    -webkit-transform: translateX(-50%) translateY(-50%);
    -moz-transform: translateX(-50%) translateY(-50%);
    transform: translateX(-50%) translateY(-50%)
}

.video-link-testimonial:after {
    content: "";
    display: block;
    position: absolute;
    left: 0;
    top: 0;
    z-index: 2;
    opacity: 0;
    width: 100%;
    height: 100%;
    background: #f3292f;
    -webkit-transition: opacity .35s ease-in-out;
    -moz-transition: opacity .35s ease-in-out;
    transition: opacity .35s ease-in-out
}

.desktop .video-link-testimonial:hover:after {
    opacity: .65
}

.video-link-testimonial img {
    display: block;
    width: 100%;
    height: 100%;
    -webkit-transform: translateZ(0px);
    -moz-transform: translateZ(0px);
    transform: translateZ(0px);
    -webkit-transition: -webkit-transform .35s ease-in-out;
    -moz-transition: -moz-transform .35s ease-in-out;
    transition: transform .35s ease-in-out
}

.desktop .video-link-testimonial:hover img {
    -webkit-transform: scale(1.2) translateZ(0px);
    -moz-transform: scale(1.2) translateZ(0px);
    transform: scale(1.2) translateZ(0px)
}

.video-link-testimonial span {
    display: block;
    position: absolute;
    left: 0;
    top: 50%;
    opacity: 0;
    display: none;
    z-index: 3;
    width: 100%;
    text-align: center;
    color: #fff;
    -webkit-transform: translateY(85px);
    -moz-transform: translateY(85px);
    transform: translateY(85px);
    -webkit-transition: all .35s ease-in-out;
    -moz-transition: all .35s ease-in-out;
    transition: all .35s ease-in-out
}

.desktop .video-link-testimonial span {
    display: block
}

.desktop .video-link-testimonial:hover span {
    opacity: 1;
    -webkit-transform: translateY(65px);
    -moz-transform: translateY(65px);
    transform: translateY(65px)
}
